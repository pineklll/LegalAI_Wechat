/* pages/word_data/index.wxss */
.search_input {
  height: 90rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
}
.search_input input {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align:center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}

.search_input box {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align:center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}

.center{
  text-align: center;
}
